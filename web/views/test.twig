    <ul>
    {% for opinionToAnswer in user.opinionToAnswerList %}
        <li>({{ opinionToAnswer.id }} : {{ opinionToAnswer.date }} : {{ opinionToAnswer.answer }}) </li>
    {% endfor %}
    </ul>
    <br />
    <br />
{% endfor %}